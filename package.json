{
  "name": "novel-app",
  "version": "1.3.2",
  "description": "novel web app",
  "main": "app.js",
  "scripts": {
    "start": "npm run server&&npm run dev",
    "server": "nodemon app.js",
    "build": "parcel build ./web/main.js --no-minify --no-cache",
    "dev": "parcel ./web/index.html",
    "pro": "parcel build ./web/main.js --no-cache",
    "css": "parcel build ./web/css/main.css --no-cache",
    "ssr": "babel web --out-dir ssr"
  },
  "nodemonConfig": {
    "ignore": [
      "web/*",
      "dist/*"
    ],
    "delay": "2500"
  },
  "keywords": [
    "novel",
    "web",
    "js",
    "node"
  ],
  "author": "wanglin",
  "license": "ISC",
  "devDependencies": {
    "cheerio": "^1.0.0-rc.2",
    "lodash.unescape": "^4.0.1",
    "parcel-bundler": "^1.7.1",
    "request": "^2.83.0"
  },
  "dependencies": {
    "@material-ui/icons": "^1.0.0-beta.43",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "crypto-browserify": "^3.12.0",
    "ejs": "^2.5.7",
    "express": "^4.16.1",
    "express-session": "^1.15.6",
    "material-ui": "^1.0.0-beta.43",
    "moment": "^2.19.3",
    "mongoose": "^4.11.13",
    "postcss-modules": "^1.1.0",
    "react": "^16.3.2",
    "react-dom": "^16.2.0",
    "react-materialize": "^2.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "underscore": "^1.8.3"
  }
}
